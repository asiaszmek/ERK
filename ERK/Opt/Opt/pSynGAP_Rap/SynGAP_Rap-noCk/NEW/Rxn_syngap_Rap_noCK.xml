
<ReactionScheme>
<!-- Molecule species and diffusion constants -->
    <Specie name="SynGap"                id="SynGap"                kdiff="0"     kdiffunit="mu2/s"/>
    <Specie name="RapGTPGap"             id="RapGTPGap"             kdiff="0"     kdiffunit="mu2/s"/>
    <Specie name="RapGTP"                id="RapGTP"                kdiff="0"     kdiffunit="mu2/s"/>
    <Specie name="RapGDP"                id="RapGDP"                kdiff="0"     kdiffunit="mu2/s"/>
 
<!--*************************************************************************
    Now, the reactions themselves 
**************************************************************************-->
<!--(Inactivation of Rap by SynGAP) RapGTP+SynGap <-> RapGTPGap -> SynGap + RapGDP
 kcat and km from Walkup JBC 2015-->

<Reaction name = "RapGTP+SynGap--RapGTPGap" id="RapGTP+SynGap--RapGTPGap">
  <Reactant specieID="RapGTP" />
  <Reactant specieID="SynGap" />
  <Product specieID="RapGTPGap" />

  <forwardRate> 0.004222e-6</forwardRate>
  <reverseRate> 3.8e-3</reverseRate>
</Reaction>

<Reaction name = "RapGTPGap--SynGap+RasGDP" id = "RapGTPGap--SynGap+RapGDP">
  <Reactant specieID="RapGTPGap" />
  <Product specieID="SynGap" />
  <Product specieID="RapGDP" />

  <forwardRate>3.8e-3</forwardRate>
  <reverseRate> 0 </reverseRate>
</Reaction >

</ReactionScheme>

