    <Specie name="bRafRap1GTP"           id="bRafRap1GTP"           kdiff="0"     kdiffunit="mu2/s"/> 
    <Specie name="bRafRap1GTPMAPKK"      id="bRafRap1GTPMAPKK"      kdiff="0"     kdiffunit="mu2/s"/> 
