
<StimulationSet>

  <!--onset and duration in ms, rate in particles/ms-->

  
<!--fast injection to have sharp increase-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               11000          </onset>
        <duration>        10         </duration> 
        <rate>             162000       </rate>
   
	<period>        1100             </period>
	<end>          12000           </end>
	<interTrainInterval> 1000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
   
	
</InjectionStim>

<!--maintained desire concentration-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               11000          </onset>
        <duration>        10         </duration> 
        <rate>            2500       </rate>
	<period>        25         </period>
	<end>        12000        </end>
	<interTrainInterval> 1000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
	
</InjectionStim>

<!--buff to bring back Ca to basal. repeated -->
<InjectionStim specieID="Cbuff" injectionSite="pointA">

        <onset>               12000          </onset>
        <duration>        5         </duration> 
      	<rate>       460000 </rate>
	<period>        1000             </period>
	<end>          12900           </end>
	<interTrainInterval> 1000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
</InjectionStim>



<!--slow buff for external Ca, keep basal -->   
   <InjectionStim specieID="Cbuffslow" injectionSite="pointA">

        <onset>               11000          </onset>
