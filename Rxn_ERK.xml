    <Specie name="CKp"                   id="CKp"                   kdiff="0"     kdiffunit="mu2/s"/>
    <Specie name="CKCamCa4"              id="CKCamCa4"              kdiff="0"     kdiffunit="mu2/s"/>
       
<!--*************************************************************************
    Now, the reactions themselves 
    **************************************************************************-->

<!--PATHWAY 1 TO ACTIVATE ERK-->

<!--(activaton of RasGRF) CamCa4+RasGRF->CamCa4RasGRF -->
    <Reaction name = "CamCa4+RasGRF--CamCa4RasGRF" id="CamCa4+RasGRF--CamCa4RasGRF">
        <Reactant specieID="CamCa4"           />
        <Reactant specieID="RasGRF"              />
        <Product  specieID="CamCa4RasGRF"               />

        <forwardRate> 0.06e-3 </forwardRate>
        <reverseRate> 1e-3        </reverseRate>
        
    </Reaction>

<!--(active RasGRF activates Ras)CamCa4RasGRF +RasGDP <-> CamCa4RasGRFRasGDP <-> CamCa4RasGRF + RasGTP-->
    <Reaction name = "CamCa4RasGRF+RasGDP--CamCa4RasGRFRasGDP" id="CamCa4RasGRF+RasGDP--CamCa4RasGRFRasGDP">
        <Reactant specieID="CamCa4RasGRF"           />
        <Reactant specieID="RasGDP"              />
        <Product  specieID="CamCa4RasGRFRasGDP"               />

        <forwardRate> 0.000198e-3 </forwardRate>
        <reverseRate> 0.08e-3 </reverseRate>
        
    </Reaction>

    <Reaction name = "CamCa4RasGRFRasGDP--CamCa4RasGRF+RasGTP" id="CamCa4RasGRFRasGDP--CamCa4RasGRF+RasGTP">
        <Reactant specieID="CamCa4RasGRFRasGDP"               />
        <Product specieID="CamCa4RasGRF"             />
        <Product specieID="RasGTP"               />

        <forwardRate> 0.02e-3 </forwardRate>
        <reverseRate> 0 </reverseRate>
       
    </Reaction>

<!--(Ras activates bRaf) RasGTP+bRaf <-> bRafRasGTP-->
    <Reaction name = "RasGTP+bRaf--bRafRasGTP" id="RasGTP+bRaf--bRafRasGTP">
        <Reactant specieID="RasGTP"           />
        <Reactant specieID="bRaf"              />
        <Product  specieID="bRafRasGTP"               />

        <forwardRate> 0.06e-3 </forwardRate>
        <reverseRate> 0.5e-3        </reverseRate>
      
    </Reaction>

<!--(Inactivation of Ras by SynGAP) RasGTP+SynGap <-> RasGTPGap -> SynGap + RasGDP -->

<Reaction name = "RasGTP+SynGap--RasGTPGap" id="RasGTP+SynGap--RasGTPGap">
  <Reactant specieID="RasGTP" />
  <Reactant specieID="SynGap" />
  <Product specieID="RasGTPGap" />

  <forwardRate>0.02e-3</forwardRate>
  <reverseRate>6.4e-3</reverseRate>

</Reaction>

<Reaction name = "RasGTPGap--SynGap+RasGDP" id = "RasGTPGap--SynGap+RasGDP">
  <Reactant specieID="RasGTPGap" />
  <Product specieID="SynGap" />
  <Product specieID="RasGDP" />

  <forwardRate>1.6e-3</forwardRate>
  <reverseRate>0e-3</reverseRate>

</Reaction>

<!--(Inactivation of Rap by SynGAP) Rap1GTP+SynGap <-> Rap1GTPGap -> SynGap + Rap1GDP -->

<Reaction name = "Rap1GTP+SynGap--Rap1GTPGap" id="Rap1GTP+SynGap--Rap1GTPGap">
  <Reactant specieID="Rap1GTP" />
  <Reactant specieID="SynGap" />
  <Product specieID="Rap1GTPGap" />

  <forwardRate>0.02e-3 </forwardRate>
  <reverseRate>6.4e-3 </reverseRate>

</Reaction>

<Reaction name = "Rap1GTPGap--SynGap+Rap1GDP" id = "Rap1GTPGap--SynGap+Rap1GDP">
  <Reactant specieID="Rap1GTPGap" />
  <Product specieID="SynGap" />
  <Product specieID="Rap1GDP" />

  <forwardRate>1.6e-3</forwardRate>
  <reverseRate>0e-3</reverseRate>

</Reaction>

<!-- (Phosphorylation of SynGAP by CaMKII) CKpCamCa4+SynGap <-> CKpCamCa4SynGap -> CKpCamCa4+pSynGap -->
 <Reaction name = "CKpCamCa4+SynGap--CKpCamCa4SynGap" id="CKpCamCa4+SynGap--CKpCamCa4SynGap">
  <Reactant specieID="CKpCamCa4" />
  <Reactant specieID="SynGap" />
  <Product specieID="CKpCamCa4SynGap" />

  <forwardRate>0 </forwardRate>
  <reverseRate>0 </reverseRate>

 </Reaction>

 <Reaction name = "CKpCamCa4SynGap -- CKpCamCa4+pSynGap" id="CKpCamCa4SynGap -- CKpCamCa4+pSynGap">
  <Reactant specieID="CKpCamCa4SynGap" />
  <Product specieID="pSynGap" />
  <Product specieID="CKpCamCa4" />

  <forwardRate>0 </forwardRate>
  <reverseRate>0 </reverseRate>

 </Reaction>

<!--(Inactivation of Ras by pSynGAP) RasGTP+pSynGap <-> RasGTPGap -> pSynGap + RasGDP -->

<Reaction name = "RasGTP+pSynGap--RasGTPGap" id="RasGTP+pSynGap--RasGTPGap">
  <Reactant specieID="RasGTP" />
  <Reactant specieID="pSynGap" />
  <Product specieID="RasGTPGap" />

  <forwardRate>0.02e-3 </forwardRate>
  <reverseRate>6.4e-3  </reverseRate>

</Reaction>

<Reaction name = "RasGTPGap--pSynGap+RasGDP" id = "RasGTPGap--pSynGap+RasGDP">
  <Reactant specieID="RasGTPGap" />
  <Product specieID="pSynGap" />
  <Product specieID="RasGDP" />

  <forwardRate>1.6e-3 </forwardRate>
  <reverseRate>0e-3 </reverseRate>

</Reaction>
    
<!--(Inactivation of Rap by pSynGAP) Rap1GTP+pSynGap <-> Rap1GTPGap -> pSynGap + Rap1GDP -->

<Reaction name = "Rap1GTP+pSynGap--Rap1GTPGap" id="Rap1GTP+pSynGap--Rap1GTPGap">
  <Reactant specieID="Rap1GTP" />
  <Reactant specieID="pSynGap" />
  <Product specieID="Rap1GTPGap" />

  <forwardRate>0.02e-3 </forwardRate>
  <reverseRate>6.4e-3 </reverseRate>

</Reaction>

<Reaction name = "Rap1GTPGap--pSynGap+Rap1GDP" id = "Rap1GTPGap--pSynGap+Rap1GDP">
  <Reactant specieID="Rap1GTPGap" />
  <Product specieID="pSynGap" />
  <Product specieID="Rap1GDP" />

  <forwardRate>1.6e-3 </forwardRate>
  <reverseRate>0e-3 </reverseRate>

</Reaction>

<!--(MAPKK phos1 by bRaf-Ras) MAPKK + bRafRasGTP <-> bRafRasGTPMAPKK <-> pMAPKK + bRafRasGTP-->
    <Reaction name = "MAPKK+bRafRasGTP--bRafRasGTPMAPKK" id="MAPKK+bRafRasGTP--bRafRasGTPMAPKK">
        <Reactant specieID="MAPKK"           />
        <Reactant specieID="bRafRasGTP"              />
        <Product  specieID="bRafRasGTPMAPKK"               />

        <forwardRate> .00625e-3 </forwardRate>
        <reverseRate> 0.8e-3        </reverseRate>
      
    </Reaction> 
    <Reaction name = "bRafRasGTPMAPKK--pMAPKK+bRafRasGTP" id="bRafRasGTPMAPKK--pMAPKK+bRafRasGTP">
        <Reactant specieID="bRafRasGTPMAPKK"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="bRafRasGTP"          />

        <forwardRate> 0.2e-3 </forwardRate>
        <reverseRate> 0       </reverseRate>
      
    </Reaction> 

<!--(MAPKK phos2 by bRaf-Ras) pMAPKK + bRafRasGTP <-> bRafRasGTPpMAPKK <-> ppMAPKK + bRafRasGTP-->
    <Reaction name = "pMAPKK+bRafRasGTP--bRafRasGTPpMAPKK" id="pMAPKK+bRafRasGTP--bRafRasGTPpMAPKK">
        <Reactant specieID="pMAPKK"           />
        <Reactant specieID="bRafRasGTP"              />
        <Product  specieID="bRafRasGTPpMAPKK"               />

        <forwardRate> .00625e-3   </forwardRate>
        <reverseRate> 0.8e-3       </reverseRate>
       
    </Reaction> 
    <Reaction name = "bRafRasGTPpMAPKK--ppMAPKK+bRafRasGTP" id="bRafRasGTPpMAPKK--ppMAPKK+bRafRasGTP">
        <Reactant specieID="bRafRasGTPpMAPKK"           />
        <Product  specieID="ppMAPKK"               />
        <Product  specieID="bRafRasGTP"               />

        <forwardRate> 0.2e-3   </forwardRate>
        <reverseRate> 0      </reverseRate>
       
    </Reaction> 

<!--(ppMAPKK dephos1) ppMAPKK+PP2A <-> ppMAPKKPP2A <-> pMAPKK+PP2A-->
    <Reaction name = "ppMAPKK+PP2A--ppMAPKKPP2A" id="ppMAPKK+PP2A--ppMAPKKPP2A">
        <Reactant specieID="ppMAPKK"           />
        <Reactant specieID="PP2A"              />
        <Product  specieID="ppMAPKKPP2A"               />
                    

        <forwardRate> 0.00192e-3  </forwardRate>
        <reverseRate> 24e-3        </reverseRate>
       
    </Reaction> 
    <Reaction name = "ppMAPKKPP2A--pMAPKK+PP2A" id="ppMAPKKPP2A--pMAPKK+PP2A">
        <Reactant specieID="ppMAPKKPP2A"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="PP2A"               />

        <forwardRate> 6e-3   </forwardRate>
        <reverseRate> 0        </reverseRate>
       
    </Reaction> 

<!--(ppMAPKK dephos2) pMAPKK+PP2A <-> pMAPKKPP2A <-> MAPKK+PP2A-->
    <Reaction name = "pMAPKK+PP2A--pMAPKKPP2A" id="pMAPKK+PP2A--pMAPKKPP2A">
        <Reactant specieID="pMAPKK"           />
        <Reactant specieID="PP2A"              />
        <Product  specieID="pMAPKKPP2A"               />

        <forwardRate> 0.00191e-3   </forwardRate>
        <reverseRate> 24e-3        </reverseRate>
      
    </Reaction> 
    <Reaction name = "pMAPKKPP2A--MAPKK+PP2A" id="pMAPKKPP2A--MAPKK+PP2A">
        <Reactant specieID="pMAPKKPP2A"           />
        <Product  specieID="MAPKK"               />
        <Product  specieID="PP2A"               />

        <forwardRate> 6e-3  </forwardRate>
        <reverseRate> 0    </reverseRate>
      
    </Reaction> 



<!--PATHWAY 2 TO ACTIVATE ERK-->

<!--(activation of Src by PKAc) PKAc + Src <-> PKAcSrc <-> PKAc + pSrc-->
    <Reaction name = "PKAc+Src--PKAcSrc" id="PKAc+Src--PKAcSrc">
        <Reactant specieID="PKAc"           />
        <Reactant specieID="Src"              />
        <Product  specieID="PKAcSrc"               /> 

        <forwardRate> 2.00e-3 </forwardRate>
        <reverseRate> 80e-3   </reverseRate>
     
    </Reaction> 
    <Reaction name = "PKAcSrc--PKAc+pSrc" id="PKAcSrc--PKAc+pSrc">
        <Reactant specieID="PKAcSrc"           />
        <Product  specieID="PKAc"              />
        <Product  specieID="pSrc"               />

        <forwardRate> 20e-3   </forwardRate>
        <reverseRate> 0        </reverseRate>
     
    </Reaction> 


<!--(inactivation of pSrc) pSrc<->Src-->
    <Reaction name = "pSrc--Src" id="pSrc--Src">
        <Reactant specieID="pSrc"           />
        <Product  specieID="Src"               />

        <forwardRate> 100e-3   </forwardRate>
        <reverseRate> 0.0        </reverseRate>
   
    </Reaction> 


<!--(activation of Cbl) Cbl + pSrc <-> CblpSrc <-> pCbl + pSrc-->
    <Reaction name = "Cbl+pSrc--CblpSrc" id="Cbl+pSrc--CblpSrc">
        <Reactant specieID="Cbl"           />
        <Reactant specieID="pSrc"           />
        <Product  specieID="CblpSrc"               />

        <forwardRate> 0.0802e-3  </forwardRate>
        <reverseRate> 0.1e-3     </reverseRate>
      
    </Reaction> 
    <Reaction name = "CblpSrc--pCbl+pSrc" id="CblpSrc--pCbl+pSrc">
        <Reactant specieID="CblpSrc"           />
        <Product specieID="pCbl"           />
        <Product  specieID="pSrc"               />

        <forwardRate> 40e-3   </forwardRate>
        <reverseRate> 0       </reverseRate>
      
    </Reaction> 

<!--(inactivation of pCbl) pCbl<->Cbl-->
    <Reaction name = "pCbl--Cbl" id="pCbl--Cbl">
        <Reactant specieID="pCbl"           />
        <Product  specieID="Cbl"               />

        <forwardRate> 10e-3   </forwardRate>
        <reverseRate> 0     </reverseRate>
     
    </Reaction>


<!--(activation of CRK step2) CRKC3G + pCbl <=> CRKC3GpCbl -->
    <Reaction name = "CRKC3G+pCbl--CRKC3GpCbl" id="CRKC3G+pCbl--CRKC3GpCbl">
        <Reactant specieID="CRKC3G"           />
        <Reactant specieID="pCbl"              />
        <Product  specieID="CRKC3GpCbl"               />

        <forwardRate> 0.001e-3  </forwardRate>
        <reverseRate> 0.2e-3    </reverseRate>
      
    </Reaction> 

<!--TWO WAYS TO ACTIVATE Rap1-->

<!--(Rap activation by Crk (GEF)) Rap1GDP + CRKC3GpCbl <-> CRKC3GpCblRap1 <-> Rap1GTP + CRKC3GpCbl-->
    <Reaction name = "Rap1GDP+CRKC3GpCbl--CRKC3GpCblRap1" id="Rap1GDP+CRKC3GpCbl--CRKC3GpCblRap1">
        <Reactant specieID="Rap1GDP"           />
        <Reactant specieID="CRKC3GpCbl"              />
        <Product  specieID="CRKC3GpCblRap1"               />

        <forwardRate> 0.04e-3   </forwardRate>
        <reverseRate> 0.2e-3        </reverseRate>
     
    </Reaction> 
    <Reaction name = "CRKC3GpCblRap1--Rap1GTP+CRKC3GpCbl" id="CRKC3GpCblRap1--Rap1GTP+CRKC3GpCbl">
        <Reactant specieID="CRKC3GpCblRap1"           />
        <Product  specieID="Rap1GTP"               />
        <Product  specieID="CRKC3GpCbl"               />

        <forwardRate> 0.2e-3   </forwardRate>
        <reverseRate> 0       </reverseRate>
     
    </Reaction> 
<!--(Epac binding) Epac + camp <-> Epacamp-->
    <Reaction name = "Epac+camp--Epacamp" id="Epac+camp--Epacamp">
        <Reactant specieID="Epac"           />
        <Reactant  specieID="camp"               />
        <Product  specieID="Epacamp"               />

        <forwardRate> 0.000234e-3   </forwardRate>
        <reverseRate> 0.28e-3       </reverseRate>
     
    </Reaction>     

<!--(Epac(GEF) activates Rap1) Epacamp + Rap1<-> EpacRap1 <-> Epacamp + Rap1GTP-->
    <Reaction name = "Epacamp+Rap1GDP--EpacRap1" id="Epacamp+Rap1--EpacRap1">
        <Reactant specieID="Epacamp"           />
        <Reactant specieID="Rap1GDP"              />
        <Product  specieID="EpacRap1"               />

        <forwardRate> 0.012e-3   </forwardRate>
        <reverseRate> 0.96e-3        </reverseRate>
     
    </Reaction> 
    <Reaction name = "EpacRap1--Epacamp+Rap1GTP" id="EpacRap1--Epacamp+Rap1GTP">
        <Reactant specieID="EpacRap1"           />
        <Product  specieID="Epacamp"               />
        <Product  specieID="Rap1GTP"               />

        <forwardRate> 0.024e-3   </forwardRate>
        <reverseRate>  0      </reverseRate>
     
    </Reaction> 
<!--(inactivation of Rap1 without Gap) Rap1GTP <-> Rap1GDP-->
    <Reaction name = "Rap1GTP--Rap1GDP" id="Rap1GTP--Rap1GDP">
        <Reactant specieID="Rap1GTP"           />
        <Product  specieID="Rap1GDP"               />

        <forwardRate> .0001e-3   </forwardRate>
        <reverseRate> 0        </reverseRate>
      
    </Reaction> 

<!--(inactivation of Rap1) Rap1GTP +Rap1Gap <-> Rap1GTPGap <-> Rap1Gap+Rap1GDP-->
