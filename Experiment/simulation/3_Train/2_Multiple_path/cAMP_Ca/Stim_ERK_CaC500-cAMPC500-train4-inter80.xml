
<StimulationSet>

  <!--onset and duration in ms, rate in particles/ms-->

     <!-- Ca -->
<!--fast injection to have sharp increase-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               11000          </onset>
        <duration>        10         </duration> 
        <rate>             82000       </rate>
   
	<period>        1100             </period>
	<end>          12000           </end>
	<interTrainInterval> 80000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
   
	
</InjectionStim>

<!--maintained desire concentration-->
<InjectionStim specieID="Ca" injectionSite="pointA">
  
        <onset>               11000          </onset>
        <duration>        10         </duration> 
        <rate>             1250       </rate>
	<period>        25         </period>
	<end>        12000        </end>
	<interTrainInterval> 80000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
	
</InjectionStim>

<!--buff to bring back Ca to basal. repeated -->
<InjectionStim specieID="Cbuff" injectionSite="pointA">

        <onset>               12000          </onset>
        <duration>        5         </duration> 
      	<rate>       230000 </rate>
	<period>        1000             </period>
	<end>          12900           </end>
	<interTrainInterval> 80000     </interTrainInterval>
	<numTrains>           4       </numTrains>
   
</InjectionStim>



