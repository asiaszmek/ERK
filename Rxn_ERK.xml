    <Specie name="bRafRap1GTPGap"        id="bRafRap1GTPGap"        kdiff="0"     kdiffunit="mu2/s"/>
    <Specie name="bRafRap1Gap"           id="bRafRap1Gap"           kdiff="0"     kdiffunit="mu2/s"/>
