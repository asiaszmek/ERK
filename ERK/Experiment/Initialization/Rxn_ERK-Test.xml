<!--****************************************************************************
    Now, the reactions themselves
    ****************************************************************************-->

<!--ERK ACTIVATION BY ppMAPKK-->


    <!--(ERK phos by ppMAPKK) ppMAPKK+ERK <-> ppMAPKKERK <-> pERK+ppMAPKK-->
    <Reaction name = "ppMAPKK+ERK--ppMAPKKERK" id="ppMAPKK+ERK--ppMAPKKERK">
        <Reactant specieID="ppMAPKK"           />
        <Reactant specieID="ERK"              />
        <Product  specieID="ppMAPKKERK"               />

        <forwardRate> 0.032e-3   </forwardRate>
        <reverseRate> 1.200e-3   </reverseRate>
      
    </Reaction>
    
    <Reaction name = "ppMAPKKERK--pERK+ppMAPKK" id="ppMAPKKERK--pERK+ppMAPKK">
        <Reactant specieID="ppMAPKKERK"           />
        <Product  specieID="pERK"               />
        <Product  specieID="ppMAPKK"               />

        <forwardRate> 0.3e-3   </forwardRate>
        <reverseRate> 0        </reverseRate>
      
    </Reaction>
    
<!--(pERK phos by ppMAPKK) ppMAPKK+pERK <-> ppMAPKKpERK <-> ppERK+ppMAPKK-->
    <Reaction name = "ppMAPKK+pERK--ppMAPKKpERK" id="ppMAPKK+pERK--ppMAPKKpERK">
        <Reactant specieID="ppMAPKK"           />
        <Reactant specieID="pERK"              />
        <Product  specieID="ppMAPKKpERK"               />

       <forwardRate> 0.032e-3   </forwardRate>
       <reverseRate> 1.200e-3   </reverseRate>
      
    </Reaction>
    <Reaction name = "ppMAPKKpERK--ppERK+ppMAPKK" id="ppMAPKKpERK--ppERK+ppMAPKK">
        <Reactant specieID="ppMAPKKpERK"           />
        <Product specieID="ppERK"              />
        <Product  specieID="ppMAPKK"               />

       <forwardRate> 0.3e-3   </forwardRate>
       <reverseRate> 0        </reverseRate>
      
    </Reaction>
    
<!--ERK INACTIVATION BY MKP1-->

<!--(ppERK dephos by MKP1) ppERK+MKP1<-> ppERKMKP1 <-> pERK+MKP1-->
    <Reaction name = "ppERK+MKP1--ppERKMKP1" id="ppERK+MKP1--ppERKMKP1">
        <Reactant specieID="ppERK"           />
        <Reactant specieID="MKP1"              />
        <Product  specieID="ppERKMKP1"               />

        <forwardRate> 0.150e-3  </forwardRate>
        <reverseRate> 16e-3  </reverseRate>
        
    </Reaction>
    <Reaction name = "ppERKMKP1--pERK+MKP1" id="ppERKMKP1--pERK+MKP1">
        <Reactant specieID="ppERKMKP1"           />
        <Product  specieID="pERK"               />
        <Product  specieID="MKP1"               />

        <forwardRate> 4e-3  </forwardRate>
        <reverseRate> 0  </reverseRate>
        
    </Reaction>

<!--(pERK dephos by MKP1) pERK+MKP1<-> pERKMKP1 <-> ERK+MKP1-->
    <Reaction name = "pERK+MKP1--pERKMKP1" id="pERK+MKP1--pERKMKP1">
        <Reactant specieID="pERK"           />
        <Reactant specieID="MKP1"              />
        <Product  specieID="pERKMKP1"               />

        <forwardRate> 0.150e-3  </forwardRate>
        <reverseRate> 16e-3  </reverseRate>
        
       
    </Reaction>
    <Reaction name = "pERKMKP1--ERK+MKP1" id="pERKMKP1--ERK+MKP1">
        <Reactant specieID="pERKMKP1"           />
        <Product  specieID="ERK"               />
        <Product  specieID="MKP1"               />

        <forwardRate> 4e-3   </forwardRate>
        <reverseRate> 0   </reverseRate>
       
       </Reaction>


    <!--MAPKK ACTIVATION BY bRafRap1GTP-->
    
<!--(MAPKK phos by bRafRap1) MAPKK + bRafRap1GTP <-> bRafRap1GTPMAPKK <-> pMAPKK + bRafRap1GTP-->
    <Reaction name = "MAPKK+bRafRap1GTP--bRafRap1GTPMAPKK" id="MAPKK+bRafRap1GTP--bRafRap1GTPMAPKK">
        <Reactant specieID="MAPKK"           />
        <Reactant specieID="bRafRap1GTP"              />
        <Product  specieID="bRafRap1GTPMAPKK"               />

        <forwardRate> 0.009375e-3  </forwardRate>
        <reverseRate> 1.200000e-3  </reverseRate>
	
    </Reaction>
    
    <Reaction name = "bRafRap1GTPMAPKK--pMAPKK+bRafRap1GTP" id="bRafRap1GTPMAPKK--pMAPKK+bRafRap1GTP">
        <Reactant specieID="bRafRap1GTPMAPKK"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="bRafRap1GTP"               />

        <forwardRate> 0.3e-3 </forwardRate>
        <reverseRate> 0      </reverseRate>
      
    </Reaction>

<!--(pMAPKK phos by bRafRap1) pMAPKK + bRafRap1GTP <-> bRafRap1GTPpMAPKK <-> ppMAPKK + bRafRap1GTP-->
    <Reaction name = "pMAPKK+bRafRap1GTP--bRafRap1GTPpMAPKK" id="pMAPKK+bRafRap1GTP--bRafRap1GTPpMAPKK">
        <Reactant specieID="pMAPKK"           />
        <Reactant specieID="bRafRap1GTP"              />
        <Product  specieID="bRafRap1GTPpMAPKK"               />

        <forwardRate> 0.009375e-3  </forwardRate>
        <reverseRate> 1.200000e-3  </reverseRate>
	
    </Reaction>
    <Reaction name = "bRafRap1GTPpMAPKK--ppMAPKK+bRafRap1GTP" id="bRafRap1GTPpMAPKK--ppMAPKK+bRafRap1GTP">
        <Reactant specieID="bRafRap1GTPpMAPKK"           />
        <Product  specieID="ppMAPKK"               />
        <Product  specieID="bRafRap1GTP"               />

         <forwardRate> 0.3e-3 </forwardRate>
        <reverseRate> 0      </reverseRate>
 
    </Reaction>
    
      <!--MAPKK ACTIVATION BY pbRafRapGTP-->
    
<!--(MAPKK phos by pbRafRap1GTP) MAPKK + pbRafRap1GTP <-> pbRafRap1GTPMAPKK <-> pMAPKK + pbRafRap1GTP-->
    <Reaction name = "MAPKK+pbRafRap1GTP--pbRafRap1GTPMAPKK" id="MAPKK+pbRafRap1GTP--pbRafRap1GTPMAPKK">
        <Reactant specieID="MAPKK"           />
        <Reactant specieID="pbRafRap1GTP"              />
        <Product  specieID="pbRafRap1GTPMAPKK"               />

       <forwardRate> 0.009375e-3  </forwardRate>
        <reverseRate> 1.200000e-3  </reverseRate>
      
    </Reaction>
    <Reaction name = "pbRafRap1GTPMAPKK--pMAPKK+pbRafRap1GTP" id="pbRafRap1GTPMAPKK--pMAPKK+pbRafRap1GTP">
        <Reactant specieID="pbRafRap1GTPMAPKK"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="pbRafRap1GTP"               />

        <forwardRate> 0.3e-3   </forwardRate>
        <reverseRate> 0  </reverseRate>
      
    </Reaction>
    
    <!--(pMAPKK phos by pRaf1Rap1GTP) pMAPKK+pbRafRap1GTP <->pbRafRap1GTPMAPKK <-> ppMAPKK + pbRafRap1GTP -->
    
    <Reaction name = "pMAPKK+pbRafRap1GTP--pbRafRap1GTPpMAPKK" id="pMAPKK+pbRafRap1GTP--pbRafRap1GTPpMAPKK">
        <Reactant specieID="pMAPKK"           />
        <Reactant specieID="pbRafRap1GTP"              />
        <Product  specieID="pbRafRap1GTPpMAPKK"               />

       <forwardRate> 0.00942863e-3  </forwardRate>
        <reverseRate> 1.2e-3  </reverseRate>
      
    </Reaction>
    <Reaction name = "pbRafRasGTPpMAPKK--ppMAPKK+pbRaf1Rap1GTP" id="pbRafRap1GTPpMAPKK--ppMAPKK+pbRafRap1GTP">
        <Reactant specieID="pbRafRap1GTPpMAPKK"           />
        <Product  specieID="ppMAPKK"               />
        <Product  specieID="pbRafRap1GTP"               />

        <forwardRate> 0.3e-3   </forwardRate>
        <reverseRate> 0  </reverseRate>
      
    </Reaction>
    


<!--MAPKK INACTIVATION BY PP2A-->
    
<!--(ppMAPKK dephos by PP2A) ppMAPKK+PP2A <-> ppMAPKKPP2A <-> pMAPKK+PP2A-->
    <Reaction name = "ppMAPKK+PP2A--ppMAPKKPP2A" id="ppMAPKK+PP2A--ppMAPKKPP2A">
        <Reactant specieID="ppMAPKK"           />
        <Reactant specieID="PP2A"              />
        <Product  specieID="ppMAPKKPP2A"               />
                    

        <forwardRate> 0.0019168e-3  </forwardRate>
        <reverseRate> 24.000000e-3  </reverseRate>
       
    </Reaction>
    <Reaction name = "ppMAPKKPP2A--pMAPKK+PP2A" id="ppMAPKKPP2A--pMAPKK+PP2A">
        <Reactant specieID="ppMAPKKPP2A"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="PP2A"               />

        <forwardRate> 6e-3   </forwardRate>
        <reverseRate> 0  </reverseRate>
       
    </Reaction>

<!--(pMAPKK dephos by PP2A) pMAPKK+PP2A <-> pMAPKKPP2A <-> MAPKK+PP2A-->
    <Reaction name = "pMAPKK+PP2A--pMAPKKPP2A" id="pMAPKK+PP2A--pMAPKKPP2A">
        <Reactant specieID="pMAPKK"           />
        <Reactant specieID="PP2A"              />
        <Product  specieID="pMAPKKPP2A"               />

       <forwardRate> 0.0019168e-3  </forwardRate>
        <reverseRate> 24.000000e-3  </reverseRate>
       
    </Reaction>
    <Reaction name = "ppMAPKKPP2A--pMAPKK+PP2A" id="ppMAPKKPP2A--pMAPKK+PP2A">
        <Reactant specieID="ppMAPKKPP2A"           />
        <Product  specieID="pMAPKK"               />
        <Product  specieID="PP2A"               />

        <forwardRate> 6e-3   </forwardRate>
        <reverseRate> 0  </reverseRate>
       
    </Reaction>

    <!-- Rap1GTP activates bRaf -->
    <!-- (bRaf activation) Rap1GTP + bRaf <-> bRafRap1GTP -->
    <Reaction name = "Rap1GTP+bRaf--bRafRap1GTP" id="Rap1GTP+bRaf--bRafRap1GTP">
        <Reactant specieID="Rap1GTP"           />
        <Reactant  specieID="bRaf"               />
        <Product  specieID="bRafRap1GTP"               />

       <forwardRate> 0.06e-3   </forwardRate>
       <reverseRate> 0.50e-3   </reverseRate>
    </Reaction>


 <!-- Rap1GTP forms an inactif complex with Raf1 -->
    <!-- (Raf1 complex with Rap1) Rap1GTP + Raf1 <-> Raf1Rap1 -->
    <Reaction name = "Rap1GTP+Raf1--Raf1Rap1" id="Rap1GTP+Raf1--Raf1Rap1">
        <Reactant specieID="Rap1GTP"           />
        <Reactant  specieID="Raf1"               />
        <Product  specieID="Raf1Rap1"               />

       <forwardRate> 0.006e-3   </forwardRate>
       <reverseRate> 0.5e-3   </reverseRate>
    </Reaction>


 <!-- (PKAc phospho bRaf) PKAc + bRaf <-> PKAc + pbRaf -->
    <Reaction name = "PKAc+bRaf--bRafPKAc" id="PKAc+bRaf--bRafPKAc">
        <Reactant specieID="PKAc"           />
        <Reactant  specieID="bRaf"               />
        <Product  specieID="bRafPKAc"               />

        <forwardRate> 0.0002999e-3   </forwardRate>
        <reverseRate> 16e-3   </reverseRate>
   </Reaction>

   <Reaction name = "bRafPKAc--PKAc+pbRaf" id="bRafPKAc--PKAc+pbRaf">
        <Reactant specieID="bRafPKAc"           />
        <Product  specieID="pbRaf"               />
        <Product  specieID="PKAc"               />

        <forwardRate>  4e-3  </forwardRate>
        <reverseRate>  0  </reverseRate>
   </Reaction>
    
    
<!-- (pbRaf1 activation by Rap1GTP) Rap1GTP + pbRaf  <-> pbRafRapGTP -->
    <Reaction name = "Rap1GTP+bRaf--pbRafRap1GTP" id="Rap1GTP+pbRaf--pbRafRap1GTP">
        <Reactant specieID="Rap1GTP"           />
        <Reactant  specieID="pbRaf"               />
        <Product  specieID="pbRafRap1GTP"               />

         <forwardRate> 0.06e-3   </forwardRate>
       <reverseRate> 0.50e-3   </reverseRate>
   </Reaction>

   <!-- RasGTP binding to bRaf -->
   
  <!-- (bRaf bind to  RasGTP) RasGTP + bRaf <-> bRafRasGTP -->
    <Reaction name = "RasGTP+Raf1--bRafRasGTP" id="RasGTP+bRaf--bRafRasGTP">
        <Reactant specieID="RasGTP"           />
        <Reactant  specieID="bRaf"               />
        <Product  specieID="bRafRasGTP"               />

        <forwardRate> 0.006e-3   </forwardRate>
        <reverseRate> 1e-3   </reverseRate>
    </Reaction>
    
    
  <!--Two pathways to activate Rap1-->
    
    <!--Epac pathway to activate Rap1-->
    
   <!--(Epac binding) Epac + cAMP <-> EpacAMP-->
    <Reaction name = "Epac+cAmp--EpacAMP" id="Epac+cAMP--EpacAMP">
        <Reactant specieID="Epac"           />
        <Reactant  specieID="cAMP"               />
        <Product  specieID="EpacAMP"               />

        <forwardRate> 0.00234e-3   </forwardRate>
